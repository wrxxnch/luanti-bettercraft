msgid "@1 went up in flames"
msgstr "@1 suak hartu du"

msgid "@1 walked into fire whilst fighting @2"
msgstr "@1 sutara joan da @2ren aurka borrokan ari zela"

msgid "@1 was struck by lightning"
msgstr "@1 tximista batek jo du"

msgid "@1 was struck by lightning whilst fighting @2"
msgstr "@1 tximista batek jo du @2rekin borrokan ari zela"

msgid "@1 burned to death"
msgstr "@1 erre egin da hil arte"

msgid "@1 was burnt to a crisp whilst fighting @2"
msgstr "@1 erre egin da hil arte, @2ren aurka borrokan ari zela"

msgid "@1 tried to swim in lava"
msgstr "@1 laban igeri egiten saiatu da"

msgid "@1 tried to swim in lava to escape @2"
msgstr "@1 laban igeri egiten saiatu da @2tik ihes egiteko"

msgid "@1 discovered the floor was lava"
msgstr "@1 lurra laba dela ohartu da"

msgid "@1 walked into danger zone due to @2"
msgstr "@1 arrisku-eremu batean sartu da @2-ren erruz"

msgid "@1 suffocated in a wall"
msgstr "@1 pareta batean ito da"

msgid "@1 suffocated in a wall whilst fighting @2"
msgstr "@1 pareta batean ito da @2ren aurka borrokan ari zela"

msgid "@1 drowned"
msgstr "@1 ito egin da"

msgid "@1 drowned whilst trying to escape @2"
msgstr "@1 ito egin da @2tik ihes egin nahian zebilela"

msgid "@1 starved to death"
msgstr "@1 gosez hil da"

msgid "@1 starved to death whilst fighting @2"
msgstr "@1 gosez hil da @2ren aurka borrokan ari zela"

msgid "@1 was pricked to death"
msgstr "@1 ziztatuta hil da"

msgid "@1 walked into a cactus whilst trying to escape @2"
msgstr "@1 kaktusera joan da, @2tik ihes egiten ari zela"

msgid "@1 hit the ground too hard"
msgstr "@1(e)k lurra oso gogor jo du"

msgid "@1 hit the ground too hard whilst trying to escape @2"
msgstr "@1(e)k lurra oso gogor jo du, @2tik ihes egiten ari zela"

msgid "@1 experienced kinetic energy"
msgstr "@1(e)k energia zinetikoarekin esperimentatu du"

msgid "@1 experienced kinetic energy whilst trying to escape @2"
msgstr "@1(e)k energia zinetikoarekin esperimentatu du, @2tik ihes egiten ari zela"

msgid "@1 fell out of the world"
msgstr "@1 mundutik erori da"

msgid "@1 didn't want to live in the same world as @2"
msgstr "@1(e)k ez nuen @2-ren mundu berean bizi nahi"

msgid "@1 died"
msgstr "@1 hil egin da"

msgid "@1 died because of @2"
msgstr "@1 @2ren erruz hil da"

msgid "@1 was killed by magic"
msgstr "@1 magiak jota hil da"

msgid "@1 was killed by magic whilst trying to escape @2"
msgstr "@1 magiak jota hil da, @2tik ihes egin nahian zebilela"

msgid "@1 was killed by @2 using magic"
msgstr "@2(e)k magia erabiliz hil du @1"

msgid "@1 was killed by @2 using @3"
msgstr "@2(e)k @1 hil du @3 erabiliz"

msgid "@1 was roasted in dragon breath"
msgstr "@1 dragoi-hatsarekin hil da"

msgid "@1 was roasted in dragon breath by @2"
msgstr "@1 dragoi-hatsarekin hil du @2(e)k"

msgid "@1 withered away"
msgstr "@1 zimeldu egin da"

msgid "@1 withered away whilst fighting @2"
msgstr "@1 @2ren aurka borrokan ari zela zimeldu da"

msgid "@1 was shot by a skull from @2"
msgstr "@2(e)k jaurtitako kaskezur baten ondorioz hil da @1"

msgid "@1 was squashed by a falling anvil"
msgstr "@1 ingude batek zapaldu du gainean erorita"

msgid "@1 was squashed by a falling anvil whilst fighting @2"
msgstr "@1 ingude batek zapaldu du @2ren aurka borrokan ari zela"

msgid "@1 was squashed by a falling block"
msgstr "@1 bloke batek zapaldu du gainean erorita"

msgid "@1 was squashed by a falling block whilst fighting @2"
msgstr "@1 bloke batek zapaldu du gainean erorita @2ren aurka borrokan ari zela"

msgid "@1 was slain by @2"
msgstr "@1 @2(e)k hil du"

msgid "@1 was slain by @2 using @3"
msgstr "@1 @2(e)k hil du @3 erabiliz"

msgid "@1 was shot by @2"
msgstr "@1(e)k @2ren tiroa jaso du"

msgid "@1 was shot by @2 using @3"
msgstr "@1(e)k @2ren tiroa jaso du, @3 erabiliz"

msgid "@1 was spitballed by @2"
msgstr "@1 @2(e)k bota du"

msgid "@1 was spitballed by @2 using @3"
msgstr "@1(r)i @2(e)k bota da @3 erabiliz"

msgid "@1 was fireballed by @2"
msgstr "@2(e)k su-bola bat jaurti dio @1(r)i"

msgid "@1 was fireballed by @2 using @3"
msgstr "@2(e)k su-bola bat jaurti dio @1(r)i, @3 erabiliz"

msgid "@1 was killed trying to hurt @2"
msgstr "@1 hil egin da @2 kaltetu nahian"

msgid "@1 tried to hurt @2 and died by @3"
msgstr "@1 @2 zauritzen saiatu da eta @3-gatik hil da"

msgid "@1 blew up"
msgstr "@1 lehertu egin da"

msgid "@1 was blown up by @2"
msgstr "@2(e)k @1 hegarazi du"

msgid "@1 was blown up by @2 using @3"
msgstr "@2(e)k @1 hegarazi du @3 erabiliz"

msgid "@1 was squished too much"
msgstr "@1(e)k presa pixka bat zuek"

msgid "@1 was squashed by @2"
msgstr "@2(e)k @1 zapaldu da"

msgid "@1 went off with a bang"
msgstr "Zerbait lehertu zaio @1(r)i"

msgid "@1 went off with a bang due to a firework fired by @2 from @3"
msgstr "Zerbait lehertu zaio @1(r)i @2(e)k @3tikjaurtitako su artifizial baten ondorioz"

msgid "@1 was poked to death by a sweet berry bush"
msgstr "@1 baia gozozko zuhaixka baten ondorioz hil da"

msgid "@1 was poked to death by a sweet berry bush whilst trying to escape @2"
msgstr "@1 baia gozozko zuhaixka baten ondorioz hil da @2tik ihes egin nahian zebilela"

